<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape
            android:shape="rectangle">
            <solid android:color="#D5D5D5" />
            <corners
                android:radius="12dp"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/basic_bg_gray" />
                <stroke
                    android:width="1px"
                    android:color="@color/basic_bg_gray" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape
                android:shape="rectangle">
                <solid android:color="#FFFFCD8D" />
                <corners
                    android:radius="12dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
